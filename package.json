{
  "name": "bootvue",
  "description": "Vue Bootstrap components",
  "version": "0.0.1",
  "packageManager": "pnpm@9.14.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zangetsu002/bootvue.git"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nuxt-module-build build", 
    "prepack": "pnpm build",

    "dev": "nuxi dev playground",
    "dev:vue": "pnpm --filter playground-vue dev",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare docs && vite build playground-vue",
    
    "docs": "nuxi dev docs",
    "docs:build": "nuxi build docs",
    "docs:prepare": "nuxt-component-meta docs",
    
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxi typecheck playground && nuxi typecheck docs && cd playground-vue && vue-tsc --noEmit",
    "test": "vitest",
    "test:vue": "vitest -c vitest.vue.config.ts",
    "test:vue:build": "vite build playground-vue",
    "release": "release-it --preRelease=alpha --npm.tag=next"
  },
  "dependencies": {
    "@internationalized/date": "^3.6.0",
    "@internationalized/number": "^3.6.0",
    "@nuxt/devtools-kit": "^1.6.1",
    "@nuxt/kit": "^3.14.1592",
    "@nuxt/schema": "^3.14.1592",
    "@nuxtjs/color-mode": "^3.5.2",
    "@vueuse/core": "^12.0.0",
    "@vueuse/integrations": "^12.0.0",
    "unplugin": "^1.16.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5"
  },

  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/test-utils": "^3.14.4",
    "@release-it/conventional-changelog": "^9.0.3",
    "@standard-schema/spec": "1.0.0-beta.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.16.0",
    "happy-dom": "^15.7.4",
    "nuxt": "^3.14.1592",
    "nuxt-component-meta": "^0.9.0",
    "release-it": "^17.10.0",
    "superstruct": "^2.0.2",
    "valibot": "^0.42.1",
    "vitest": "^2.1.6",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-tsc": "^2.1.10",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  }
}
